    <!--<directive name="script-src">unsafe-eval</directive> &lt;!&ndash; XXX funktionioert noch nicht &ndash;&gt;-->

  </content-security-policy>

  <theme-definitions>
    <theme-definition>
      <name>astro</name>
      <display-name>Astro Theme</display-name>
      <fallback>roxborough</fallback>
      <version>1.0</version>
    </theme-definition>
  </theme-definitions>


</tobago-config>
